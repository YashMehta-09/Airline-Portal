package com.airline.model;

import com.airline.model.FlightDetails;

public class FlightBo {
	
	//List<FlightDetails> list=new ArrayList<>();
	
	public List<FlightDetails>  flightRecords(FlightDetails flightdetails){
		
		
		FlightRecordsDao flightdao=new FlightRecordsDao();
		
	List<FlightDetails> list=flightdao.records(flightdetails);
	
		
		return list;
		
	}

}
